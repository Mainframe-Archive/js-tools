    "test:types": "flow check",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "@mainframe/transport-create-http": "^0.1.1",
    "node-fetch": "^2.3.0"
