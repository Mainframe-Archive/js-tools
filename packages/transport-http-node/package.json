{
  "name": "@mainframe/transport-http-node",
  "version": "0.1.1",
  "description": "HTTP transport for node",
  "repository": "git@github.com:MainframeHQ/js-tools.git",
  "main": "lib/index.js",
  "author": "Mainframe",
  "license": "MIT",
  "files": [
    "lib/*"
  ],
  "scripts": {
    "clean": "del lib",
    "build:js": "BABEL_ENV=node babel src --out-dir lib",
