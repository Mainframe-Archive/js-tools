{
  "name": "@mainframe/rpc-http-node",
  "version": "0.2.0",
  "description": "HTTP RPC for node",
  "repository": "git@github.com:MainframeHQ/js-tools.git",
  "main": "lib/index.js",
  "types": "types/index.d.ts",
  "author": "Mainframe",
  "license": "MIT",
  "files": [
    "lib/*",
    "types/*"
  ],
  "scripts": {
    "clean": "del lib types",
    "build:js": "BABEL_ENV=node babel src --out-dir lib --extensions \".ts\"",
    "build:types": "tsc --emitDeclarationOnly",
    "build": "yarn clean && yarn build:js && yarn build:types",
    "test:types": "tsc --noEmit",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "@mainframe/rpc-request": "^0.2.0",
    "@mainframe/transport-http-node": "^0.2.0"
  }
}
